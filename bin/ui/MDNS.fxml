                        </CheckBox>
                        <CheckBox fx:id="mxCheckBox" mnemonicParsing="false" text="MX" GridPane.rowIndex="3">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </CheckBox>
                        <CheckBox fx:id="cnameCheckBox" mnemonicParsing="false" text="CNAME" GridPane.rowIndex="4">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </CheckBox>
                        <CheckBox fx:id="ptrCheckBox" mnemonicParsing="false" text="PTR" GridPane.rowIndex="5">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </CheckBox>
                     </children>
                  </GridPane>
               </content>
            </TitledPane>
            <TitledPane fx:id="dnssecTitledPane" animated="false" layoutX="25.0" layoutY="524.0" prefHeight="98.0" prefWidth="202.0" text="DNSSEC">
