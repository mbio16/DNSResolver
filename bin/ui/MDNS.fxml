                  <GridPane>
                    <columnConstraints>
                      <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                     <children>
                        <RadioButton fx:id="ipv4RadioButton" mnemonicParsing="false" text="IPv4">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </RadioButton>
                        <RadioButton fx:id="ipv6RadioButton" mnemonicParsing="false" text="IPv6" GridPane.rowIndex="1">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </RadioButton>
                     </children>
                  </GridPane>
               </content>
            </TitledPane>
            <TitledPane fx:id="recordTypeTitledPane" animated="false" layoutX="25.0" layoutY="257.0" prefHeight="252.0" prefWidth="202.0" text="Záznam">
